<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hailong.www.mapper.SchoolMapper">
    <select id="getSchoolById" resultType="com.hailong.www.model.School">
        select *
        from school
        where id = #{id}
    </select>
</mapper>